{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Pyconlang",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"comments": {
			"patterns": [{
				"name": "comment.part-of-speech.pycl",
				"match": "(\\(\\w+\\.\\))"
			}, {
				"name": "comment.line.pycl",
				"match": "(^#.*)"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.record.pycl",
				"match": "^(entry|affix|template|include|lang)\\b"
			}]
		},
		"variables": {
			"patterns": [{
				"name": "variable.entry.pycl",
				"match": "(<[^>]+>)"
			}, {
				"name": "variable.suffix.pycl",
				"match": "(\\.\\S+)\\b"
			}, {
				"name": "variable.prefix.pycl",
				"match": "\\b(\\S+\\.)"
			}, {
				"name": "variable.language.pycl",
				"match": "(\\$)"
			}]
		},
		"constants": {
			"patterns": [{
				"name": "constant.numeric.rule.pycl",
				"match": "(@\\S+)"
			}, {
				"name": "constant.numeric.lang.pycl",
				"match": "(%\\S+)"
			}]
		},
		"strings": {
			"patterns": [{
				"name": "string.morpheme.pycl",
				"match": "(\\*\\S+)"
			}, {
				"name": "string.single-quotes.pycl",
				"match": "'.*?'"
			}, {
				"name": "string.double-quotes.pycl",
				"match": "\".*?\""
			}]
		}
	},
	"scopeName": "source.pycl"
}