{
  "main": "./out/extension.js",
  "name": "pyconlang",
  "displayName": "Pyconlang",
  "publisher": "Pyconlang",
  "description": "Syntax highlighting for Pyconlang lexicon language",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neta-elad/pyconlang-vscode"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pycl",
      "aliases": ["Pyconlang", "pycl"],
      "extensions": [".pycl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pycl",
      "scopeName": "source.pycl",
      "path": "./syntaxes/pycl.tmLanguage.json"
    }],
    "commands": [
      {
        "command": "pyconlang.helloWorld",
        "title": "Pyconlang: Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
